{
    "name": "mizar-extension",
    "displayName": "Mizar extension",
    "description": "An extension for VS Code which provides support for the Mizar language.",
    "version": "0.2.0",
    "publisher": "fpsbpkm",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mimosa-project/emvscode"
    },
    "activationEvents": [
        "onCommand:mizar-compile",
        "onCommand:mizar-it",
        "onCommand:mizar-irrths",
        "onCommand:mizar-relinfer",
        "onCommand:mizar-trivdemo",
        "onCommand:mizar-reliters",
        "onCommand:mizar-relprem",
        "onCommand:mizar-irrvoc",
        "onCommand:mizar-inacc",
        "onLanguage:Mizar"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "category": "Mizar",
                "command": "mizar-compile",
                "title": "mizar-compile"
            },
            {
                "category": "Mizar",
                "command": "mizar-it",
                "title": "mizar-it"
            },
            {
                "category": "Mizar",
                "command": "mizar-irrths",
                "title": "mizar-irrths"
            },
            {
                "category": "Mizar",
                "command": "mizar-relinfer",
                "title": "mizar-relinfer"
            },
            {
                "category": "Mizar",
                "command": "mizar-trivdemo",
                "title": "mizar-trivdemo"
            },
            {
                "category": "Mizar",
                "command": "mizar-reliters",
                "title": "mizar-reliters"
            },
            {
                "category": "Mizar",
                "command": "mizar-relprem",
                "title": "mizar-relprem"
            },
            {
                "category": "Mizar",
                "command": "mizar-irrvoc",
                "title": "mizar-irrvoc"
            },
            {
                "category": "Mizar",
                "command": "mizar-inacc",
                "title": "mizar-inacc"
            }
        ],
        "languages": [
            {
                "id": "Mizar",
                "aliases": [
                    "Mizar",
                    "Mizar"
                ],
                "extensions": [
                    ".miz"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Mizar",
                "scopeName": "source.miz",
                "path": "./syntaxes/Mizar.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode": "^1.1.36"
    },
    "icon": "images/mizar.png",
    "dependencies": {
        "carrier": "^0.3.0",
        "fs": "0.0.1-security",
        "path": "^0.12.7",
        "readline": "^1.3.0"
    }
}
